{
  "name": "password-generator-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "browserslist": "last 2 versions",
  "scripts": {
    "dev": "vite --host",
    "build": "vite build --base=/password-generator-app/",
    "lint:js": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "lint:css": "stylelint src/**/*.css",
    "test": "jest --no-cache",
    "preview": "vite preview"
  },
  "dependencies": {
    "@acusti/webcrypto": "^1.0.2",
    "classnames": "^2.5.1",
    "clipboard-copy": "^4.0.1",
    "dedupe": "^4.0.3",
    "entropy-string": "^4.2.0",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-toastify": "^10.0.4"
  },
  "devDependencies": {
    "@babel/core": "^7.24.3",
    "@babel/preset-env": "^7.24.3",
    "@babel/preset-react": "^7.24.1",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.2",
    "@types/react": "^18.2.56",
    "@types/react-dom": "^18.2.19",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.56.0",
    "eslint-plugin-jest": "^28.2.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-transform-stub": "^2.0.0",
    "modern-normalize": "^2.0.0",
    "react-test-renderer": "^18.2.0",
    "stylelint": "^16.3.1",
    "stylelint-config-css-modules": "^4.4.0",
    "stylelint-config-standard": "^36.0.0",
    "stylelint-selector-bem-pattern": "^4.0.0",
    "vite": "^5.1.4"
  },
  "jest": {
    "verbose": true,
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "./src/setup.js"
    ],
    "transform": {
      "\\.[jt]sx?$": "babel-jest",
      ".+\\.(css|styl|less|sass|scss|png|svg|jpg|ttf|woff|woff2)$": "jest-transform-stub"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!@acusti/webcrypto)"
    ],
    "moduleNameMapper": {
      "^.+.(css|styl|less|sass|scss|png|jpg|ttf|woff|woff2)$": "jest-transform-stub"
    }
  }
}
